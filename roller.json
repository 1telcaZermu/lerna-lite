{
  "packages": [
    "packages/*"
  ],
  "version": "0.1.1",
  "command": {
    "publish": {
      "conventionalCommits": true,
      "bump": "from-package"
    },
    "version": {
      "conventionalCommits": true,
      "message": "chore(release): publish new version %v"
    }
  },
  "loglevel": "info"
}
